    "# z점수 구하기\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "x = [0,3,5,7,10]\n",
    "\n",
    "s = np.std(x)\n",
    "m = np.mean(x)\n",
    "z = (7-m) / s\n",
    "print(z)"
